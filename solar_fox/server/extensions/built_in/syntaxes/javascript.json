{
	"name": "Javascript support (Built in)",
	"description": "Adds javascript syntax highlighting",
	"developer": "ShiroDevv",
	"extension_type": "syntax",
	"languages": [
		"js",
		"mjs",
		"cjs",
		"jsx"
	],
	"break_character": {
		"character": "\\",
		"color": "#744B00"
	},
	"keywords": [
		{
			"regex": "/\\b(arguments|class|debugger|delete|export|extends|for|function|async|import|require|from|in|return|this|crypto)\\b/",
			"color": "blue"
		},
		{
			"regex": "/\\b(await|case|continue|default|else|finally|goto|if|super|switch|while|document|element)\\b/",
			"color": "darkslateblue"
		},
		{
			"regex": "/\\b(break|catch|do|try|implements|interface|new|private|protected|public|purple)\\b/",
			"color": "purple"
		},
		{
			"regex": "/\\b(eval|typeof|alert|blur|clearInterval|clearTimeout|close|decodeURI|decodeURIComponent|encodeURI|encodeURIComponent)\\b/",
			"color": "yellow"
		},
		{
			"regex": "/\\b(module|instanceof|package|confirm|focus|navigator|history|open|opener)\\b/",
			"color": "brown"
		},
		{
			"regex": "/\\b(true|false|null|closed|escape|innerHeight|innerWidth|location|mimeType|frames)\\b/",
			"color": "orange"
		},
		{
			"regex": "/\\b(let|const|var)\\b/",
			"color": "slateblue"
		},
		{
			"regex": "/\\b(throw|throws|void|yield|undefined|null|NaN)\\b/",
			"color": "red"
		},
		{
			"regex": "/\\b(Array|Date|Infinity|isNaN|length|Math|Number|Object|prototype|Strng|toString|valueOf)\\b/",
			"color": "azure"
		},
		{
			"regex": "/[;:]/",
			"color": "gray"
		},
		{
			"regex": "/[.]/",
			"color": "red"
		}
	],
	"comments": {
		"line_comment": {
			"keyword": "//"
		},
		"block_comment": {
			"start_keyword": "/*",
			"end_keyword": "*/"
		},
		"color": "green"
	},
	"quotes": {
		"quote_strings": [
			"\"",
			"'",
			"`"
		],
		"color": "chocolate"
	},
	"enabled": true
}